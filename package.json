{
    "name": "@patik/dof",
    "version": "2.1.0",
    "private": true,
    "workspaces": [
        "package",
        "app"
    ],
    "scripts": {
        "dev": "yarn --cwd package tsc && yarn --cwd app dev",
        "start": "yarn --cwd app start",
        "start:ci": "yarn build && yarn --cwd app start",
        "build": "yarn --cwd package lint && yarn --cwd package tsc && yarn --cwd app build",
        "clean": "git clean -fdx",
        "cover": "yarn --cwd package test --collectCoverage",
        "lint": "yarn workspaces run lint",
        "preversion": "yarn workspaces run preversion",
        "postversion": "git push && git push --tags",
        "test": "yarn workspaces run test",
        "cypress:open": "cypress open"
    },
    "dependencies": {
        "typescript": "^4.9.4"
    },
    "devDependencies": {
        "@jest/globals": "^29.3.1",
        "@types/jest": "^29.2.3",
        "@types/node": "18.11.13",
        "@types/react": "18.0.26",
        "@types/react-dom": "18.0.9",
        "@typescript-eslint/eslint-plugin": "^5.44.0",
        "@typescript-eslint/parser": "^5.44.0",
        "cypress": "^12.2.0",
        "eslint": "8.29.0",
        "eslint-config-next": "13.0.6",
        "eslint-plugin-cypress": "^2.12.1",
        "eslint-plugin-jsx-a11y": "^6.6.1",
        "jest": "^29.3.1",
        "ts-jest": "^29.0.3",
        "ts-node": "^10.9.1",
        "ts-prune": "^0.10.3"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/patik/dof.git"
    }
}
