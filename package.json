{
    "name": "@patik/dof",
    "private": true,
    "workspaces": [
        "package",
        "app"
    ],
    "scripts": {
        "dev": "yarn --cwd app next dev",
        "start": "yarn --cwd app next start",
        "build": "yarn --cwd package lint && yarn --cwd package tsc && yarn --cwd app next build",
        "clean": "git clean -fdx",
        "cover": "yarn --cwd package test --collectCoverage",
        "lint": "yarn workspaces run lint",
        "preversion": "yarn workspaces run preversion",
        "postversion": "git push && git push --tags",
        "test": "yarn workspaces run test"
    },
    "devDependencies": {
        "typescript": "^4.9.4"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/patik/dof.git"
    }
}
